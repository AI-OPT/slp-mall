<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="routeDao" class="com.ai.slp.route.cache.route.dao.impl.RouteDaoImpl"></bean>
    <bean id="routeGroupDao" class="com.ai.slp.route.cache.route.dao.impl.RouteGroupDaoImpl"></bean>
    <bean id="routeRuleDao" class="com.ai.slp.route.cache.route.dao.impl.RouteRuleDaoImpl"></bean>


    <bean id="routeService" class="com.ai.slp.route.cache.route.service.impl.RouteServiceImpl">
        <property name="routeDao" ref="routeDao"></property>
        <property name="routeRuleDao" ref="routeRuleDao"></property>
    </bean>

    <bean id="routeRuleService" class="com.ai.slp.route.cache.route.service.impl.RouteRuleServiceImpl">
        <property name="routeRuleDao" ref="routeRuleDao"></property>
    </bean>

    <bean id="routeGroupService" class="com.ai.slp.route.cache.route.service.impl.RouteGroupServiceImpl">
        <property name="routeRuleDao" ref="routeRuleDao"></property>
        <property name="routeDao" ref="routeDao"></property>
        <property name="routeGroupDao" ref="routeGroupDao"></property>
    </bean>

    <bean id="routeCache" class="com.ai.slp.route.cache.route.RouteCacheImpl">
        <property name="routeService" ref="routeService"></property>
        <property name="routeGroupService" ref="routeGroupService"/>
        <property name="routeRuleService" ref="routeRuleService"/>
    </bean>
</beans>